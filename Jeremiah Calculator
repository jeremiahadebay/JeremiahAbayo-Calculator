let num1 = parseInt(prompt("Enter your first digit"));
let num2 = parseInt(prompt("Enter your second digit"));
let operator = prompt("Enter operator from list below: +, -, *, /");
if (operator === "+") {
  alert(num1 + num2);
} else if (operator === "-") {
  alert(num1 - num2);
} else if (operator === "*") {
  alert(num1 * num2);
} else if (operator === "/") {
  alert(num1 / num2);
} else {
  alert(num1 % num2);
}
